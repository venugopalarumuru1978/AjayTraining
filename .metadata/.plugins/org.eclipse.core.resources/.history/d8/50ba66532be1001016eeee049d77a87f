package InheritacePack;

//Super class
class Vehicle {
  Vehicle() {
      System.out.println("This is a Vehicle");
  }
}

//Subclass 
class Car extends Vehicle {
  Car() {
      System.out.println("This Vehicle is Car");
  }
}



public class ConsInherit {

	public static void main(String[] args) {
		Car  car = new Car();

	}

}
