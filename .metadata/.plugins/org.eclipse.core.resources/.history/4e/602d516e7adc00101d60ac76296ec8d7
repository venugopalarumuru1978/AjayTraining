
public class arr5 {

	public static void main(String[] args) {
			int x[][] = {{10,20},{30,40,50},{60,70,80,90}};
			
			System.out.println(x[0] + " " +  x[1] + "  " + x[2] + "  " + x[3] + "  " +x[4]);
			System.out.println();
			for(int i=0;i<x.length;i++)
			{
				System.out.print(x[i] + "\t");
			}
	}

}
