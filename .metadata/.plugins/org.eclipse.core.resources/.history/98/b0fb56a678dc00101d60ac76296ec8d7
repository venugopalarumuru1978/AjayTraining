import java.util.Scanner;

public class arr4 {

	public static void main(String[] args) {
			int x[][] = new int[3][3];
			Scanner sc = new Scanner(System.in);
			
			for(int i=0;i<2;i++) // rows
			{
				for(int j=0;j<3;j++)  // cols
				{
					System.out.print(x[i][j] + "  ");
				}
				
				System.out.println(); // line break
				
			}
	}

}
