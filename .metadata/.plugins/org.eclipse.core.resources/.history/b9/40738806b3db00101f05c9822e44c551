import java.util.Scanner;

public class loopsex {

	public static void main(String[] args) {
		// loops
		
		// while loop
		
		int x=1;
		while(x<=5)
		{
			System.out.println(" Java Online Training");
			x++;
		}
		
		// for loop
	
		for(int n=1;n<=5;n++)
		{
			System.out.println(" Python Online Training");
		}

		// do while loop
		
		int y=1;
		do
		{
			System.out.println(" Java Online Training");
			y++;
		}
		while(y<=5);

		
	}
}

