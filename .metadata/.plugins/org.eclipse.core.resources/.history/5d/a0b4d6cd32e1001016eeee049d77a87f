package abstractpack;

interface Arithematic1
{
	void Addition();
	void Minus();
	
}

interface Arithematic2
{
	void Division();
	void Multiplication();
}

class Arit_Operations implements Arithematic1, Arithematic2
{

	private int x, y;
	
	public Arit_Operations(int x, int y) {
		// TODO Auto-generated constructor stub
		this.x = x;
		this.y = y;
	}
	
	@Override
	public void Addition() {
		// TODO Auto-generated method stub
		System.out.println(x+y);
	}

	@Override
	public void Minus() {
		// TODO Auto-generated method stub
		System.out.println(x-y);
	}

	@Override
	public void Division() {
		// TODO Auto-generated method stub
		System.out.println(x/y);
	}

	@Override
	public void Multiplication() {
		// TODO Auto-generated method stub
		System.out.println(x*y);
	}
}

public class interMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Arithematic1 ar = new Arit_Operations(10, 2);
		ar.Addition();
		ar.Minus();
		Arithematic2 ar2 = new Arit_Operations(10, 2);
		ar2.Division();
		ar2.Multiplication();
	}

}
