package InheritacePack;

class DemoParent
{
	protected int x, y;
	
	public void getVal(int x, int y)
	{
		this.x = x;
		this.y = y;
	}
}

class DemoChild extends DemoParent
{
	private int sum;
	
	public void putVal()
	{
		sum = x+y;
	}
}





public class SingleMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
