<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="ModelPack">
    <class name="Student" table="Student_Hibernate">
        <id name="rollno" column="rollno" type="int">
            <generator class="native"/>
        </id>
        <property name="sname" column="stdname" type="string"/>
        <property name="course" column="course" type="string"/>
    </class>
</hibernate-mapping>
